<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为UserDao接口方法提供sql语句配置 -->
<mapper namespace="com.xujd.dao.AdminInfoDao">

    <select id="selectAdminByLoginName" parameterType="java.lang.String" resultType="AdminInfo">
    SELECT * FROM admin WHERE admin_account=#{loginName}
</select>

</mapper>